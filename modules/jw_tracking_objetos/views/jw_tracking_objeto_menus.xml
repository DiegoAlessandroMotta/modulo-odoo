<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <menuitem id="menu_jw_tracking_root" name="Tracking de Objetos"
        web_icon="jw_tracking_objetos,static/description/icon.png" />
    
    <record id="action_jw_tracking_objeto_view" model="ir.actions.act_window">
        <field name="name">Objetos</field>
        <field name="res_model">jw.tracking.objeto</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="search_view_id" ref="view_jw_tracking_objeto_search" />
    </record>
    
    <menuitem id="menu_jw_tracking_objetos" name="Todos los Objetos"
        parent="menu_jw_tracking_root" action="action_jw_tracking_objeto_view"
        sequence="10" />
    
    <record id="action_jw_tracking_objeto_perdidos" model="ir.actions.act_window">
        <field name="name">Objetos Perdidos</field>
        <field name="res_model">jw.tracking.objeto</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="domain">[('estado', '=', 'perdido')]</field>
        <field name="search_view_id" ref="view_jw_tracking_objeto_search" />
    </record>
    
    <menuitem id="menu_jw_tracking_perdidos" name="Objetos Perdidos"
        parent="menu_jw_tracking_root" action="action_jw_tracking_objeto_perdidos"
        sequence="20" />
    
    <record id="action_jw_tracking_objeto_encontrados" model="ir.actions.act_window">
        <field name="name">Objetos Encontrados</field>
        <field name="res_model">jw.tracking.objeto</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="domain">[('estado', '=', 'encontrado')]</field>
        <field name="search_view_id" ref="view_jw_tracking_objeto_search" />
    </record>
    
    <menuitem id="menu_jw_tracking_encontrados" name="Objetos Encontrados"
        parent="menu_jw_tracking_root" action="action_jw_tracking_objeto_encontrados"
        sequence="30" />
    
    <record id="action_jw_tracking_objeto_reclamados" model="ir.actions.act_window">
        <field name="name">Objetos Reclamados</field>
        <field name="res_model">jw.tracking.objeto</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="domain">[('estado', '=', 'reclamado')]</field>
        <field name="search_view_id" ref="view_jw_tracking_objeto_search" />
    </record>
    
    <menuitem id="menu_jw_tracking_reclamados" name="Objetos Reclamados"
        parent="menu_jw_tracking_root" action="action_jw_tracking_objeto_reclamados"
        sequence="40" />
    
    <record id="action_jw_tracking_objeto_entregados" model="ir.actions.act_window">
        <field name="name">Objetos Entregados</field>
        <field name="res_model">jw.tracking.objeto</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="domain">[('estado', '=', 'entregado')]</field>
        <field name="search_view_id" ref="view_jw_tracking_objeto_search" />
    </record>
    
    <menuitem id="menu_jw_tracking_entregados" name="Objetos Entregados"
        parent="menu_jw_tracking_root" action="action_jw_tracking_objeto_entregados"
        sequence="50" />
</odoo>
